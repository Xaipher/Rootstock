<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Nullable>disable</Nullable>
        <TargetFrameworks>net5.0;netstandard2.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Rootstock.Mvc</Title>
        <Authors>Jeremy Ockerman</Authors>
        <Copyright>2021</Copyright>
        <PackageProjectUrl>https://github.com/Xaipher/Rootstock</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/Xaipher/Rootstock</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Rootstock.Core\Rootstock.Core.csproj" />
    </ItemGroup>

</Project>
